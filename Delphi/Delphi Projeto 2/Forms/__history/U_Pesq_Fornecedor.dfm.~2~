  inherited Q_pesq_padrao: TFDQuery
    SQL.Strings = (
      
        'select id_fornecedor, nome, endereco, numero, bairro, cidade, uf' +
        ', cep, telefone, cnpj, '
      'email, cadastro'
      'from fornecedor'
      'order by id_fornecedor')
