  inherited Q_pesq_padrao: TFDQuery
    SQL.Strings = (
      'SELECT A.ID_VENDA,'
      'A.ID_CLIENTE,'
      'B.NOME,'
      'A.ID_FORMA_PGTO,'
      'C.DESCRICAO,'
      'A.USUARIO,'
      'A.CADASTRO,'
      'A.VALOR FROM VENDA A, CLIENTE B, FORMA_PGTO C'
      'WHERE A.ID_CLIENTE = B.ID_CLIENTE'
      'AND C.ID_FORMA_PGTO=A.ID_FORMA_PGTO'
      'ORDER BY A.ID_VENDA')
